{
    "extends": [
        "react-app",
        "airbnb",
        "plugin:jsx-a11y/recommended"
    ],
    "plugins": [
        "jsx-a11y"
    ],
    "rules": {
        "react/jsx-filename-extension": [1, { "extensions": [ ".js", ".jsx"] }],
        "max-len": [2, 200, 4, {"ignoreUrls": true}],
        "comma-dangle": [
            "error",
            "never"
        ]
    }
}